<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Base.Theme.AppMaternal" parent="Theme.Material3.DayNight.NoActionBar">
        <!-- Customize your light theme here. -->
       <item name="colorPrimary">@color/primaryColor</item>
        <item name="colorPrimaryVariant">@color/primaryColorDark</item>

    </style>

    <style name="Theme.AppMaternal" parent="Base.Theme.AppMaternal" />

    <style name="Theme.AppMaternal.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="Theme.AppMaternal.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="Theme.AppMaternal.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />
</resources>